# Atlan Configuration
# Your Atlan instance base URL (without trailing slash)
ATLAN_BASE_URL=https://your-tenant.atlan.com

# Your Atlan API token for authentication
# Get this from: Settings > API Tokens in your Atlan instance
ATLAN_API_TOKEN=your_api_token_here

# AWS Configuration (Optional - for S3 integration)
# These are typically not needed for public bucket access with unsigned requests
# but may be required for private buckets or other AWS services
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# AWS_DEFAULT_REGION=us-east-1

# Database Connection Names
# These should match the connection names in your Atlan instance
POSTGRES_CONNECTION_NAME=your-postgres-connection-name
SNOWFLAKE_CONNECTION_NAME=your-snowflake-connection-name

# S3 Configuration
S3_BUCKET_NAME=your-s3-bucket-name
S3_BUCKET_ARN=arn:aws:s3:::your-s3-bucket-name
S3_CONNECTION_NAME=your-s3-connection-name
S3_PREFIX=your/s3/prefix/path/

# Cache Configuration
CACHE_EXPIRY_HOURS=24
POSTGRES_CACHE_FILE=postgres_assets_cache.json
SNOWFLAKE_CACHE_FILE=snowflake_assets_cache.json

# Logging Configuration (Optional)
# LOG_LEVEL=INFO
